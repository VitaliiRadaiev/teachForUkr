{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "t4u/reviews-slider",
  "version": "0.1.0",
  "title": "Відгуки слайдер",
  "category": "news_stories_cases_other",
  "icon": "layout",
  "description": "",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "isHide": {
      "type": "boolean",
      "default": false
    },
    "padding": {
      "type": "object",
      "default": {
        "top": "lg",
        "right": "",
        "bottom": "lg",
        "left": ""
      }
    },
    "margin": {
      "type": "object",
      "default": {
        "top": "",
        "right": "",
        "bottom": "",
        "left": ""
      }
    },
    "background": {
      "type": "string",
      "enum": [
        "bg-light-primary",
        "bg-light-primary-80"
      ],
      "default": "bg-light-primary-80"
    },
    "decor": {
      "type": "number",
      "default": 0
    },
    "preview": {
      "type": "string"
    },
    "selectedCategories": {
      "type": "array",
      "default": []
    },
    "selectedPosts": {
      "type": "array",
      "default": []
    },
    "id": {
      "type": "string"
    }
  },
  "textdomain": "reviews-slider",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "viewScript": "file:./script.js",
  "render": "file:./render.php"
}